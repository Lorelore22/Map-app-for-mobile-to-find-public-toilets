{{#if toilets}}
<h1 class="log-heads">My added toilets</h1>
{{else}}
<p>No toilets to display!</p>
{{/if}}

<div class="toilet-list">

  <div>
    {{#each toilets}}
    <div class="each-toilet">
      <a href="/toilets/{{_id}}">{{toiletType}} Toilet: {{location}}</a>
    </div>

    {{/each}}

  </div>